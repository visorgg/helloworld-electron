{
  "name": "helloworld",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "description": "helloworld",
  "repository": {
    "type": "git",
    "url": "https://github.com/visorgg/helloworld-electron.git"
  },
  "build": {
    "productName": "helloworld",
    "appId": "com.helloworld",
    "win": {
      "publisherName": "helloworld",
      "publish": {
        "provider": "s3",
        "bucket": "helloworld"
      },
      "target": "nsis"
    },
    "forceCodeSigning": false,
    "extraResources": [
      "extra_resources/*"
    ],
    "asar": true,
    "nsis": {
      "oneClick": true,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": false,
      "perMachine": true,
      "artifactName": "${productName} Installer ${version}.${ext}"
    }
  },
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "4.1.4",
    "electron-builder": "^20.11.1"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "fs-extra": "^7.0.1",
    "https": "^1.0.0",
    "iohook": "^0.3.1",
    "react-zlib-js": "^1.0.4",
    "xml2js": "^0.4.19",
    "zlib": "^1.0.5"
  },
  "iohook": {
    "targets": [
      "node-57",
      "electron-57"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64"
    ]
  }
}
